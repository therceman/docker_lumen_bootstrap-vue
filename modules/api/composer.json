{
  "name": "app/api-module",
  "description": "App :: Api Module",
  "type": "project",
  "require": {
    "php": "^7.4",
    "darkaonline/swagger-lume": "8.*",
    "laravel/lumen-framework": "v8.2.7",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.20"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
